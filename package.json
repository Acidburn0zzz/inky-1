    "assert-html-equal": "^1.0.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "gulp": "^4.0.0",
    "gulp-uglify": "^3.0.0",
